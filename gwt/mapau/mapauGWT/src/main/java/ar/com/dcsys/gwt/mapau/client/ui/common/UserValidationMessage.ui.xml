<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="res" type="ar.com.dcsys.gwt.mapau.client.ui.common.UserValidationMessageViewResources"/>
    <g:FlowPanel>
        
        <g:FlowPanel styleName="{res.style.messagePanel}">
            <g:Label ui:field="message" styleName="{res.style.message}"/>
        </g:FlowPanel>
        
        <g:FlowPanel styleName="{res.style.confirmationPanel}">
            <g:CheckBox ui:field="confirmation" styleName="{res.style.confirmation}"/>
            <g:Label ui:field="confirmationLabel" styleName="{res.style.confirmationLabel}"/>
        </g:FlowPanel>
        
        <g:FlowPanel styleName="{res.style.actionsPanel}">
			<g:Label ui:field="cancel" styleName="{res.style.cancel}"/>
			<g:FlowPanel styleName="{res.style.separator}"/>
			<g:Label ui:field="commit" styleName="{res.style.commit}"/>
        </g:FlowPanel>
        
    </g:FlowPanel>
</ui:UiBinder> 