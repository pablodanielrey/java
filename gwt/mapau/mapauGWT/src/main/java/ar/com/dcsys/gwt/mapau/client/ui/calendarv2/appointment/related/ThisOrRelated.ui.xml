<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with type="ar.com.dcsys.gwt.mapau.client.ui.calendarv2.appointment.related.ThisOrRelatedViewResources" field="res" />
	
	<g:FlowPanel styleName="{res.style.mainContainer}">
	
		<!-- Mensaje -->
		<g:FlowPanel styleName="{res.style.messagePanel}">
			<g:Label ui:field="message" styleName="{res.style.message}"/>
		</g:FlowPanel>
	
		<!-- Actions panel -->
		<g:FlowPanel styleName="{res.style.actionsPanel}">
			<g:Label ui:field="actionThis" styleName="{res.style.actionThis}"/>
			<g:FlowPanel styleName="{res.style.separator}"/>
			<g:Label ui:field="actionRelated" styleName="{res.style.actionRelated}"/>			
		</g:FlowPanel>
	
	</g:FlowPanel>
</ui:UiBinder> 