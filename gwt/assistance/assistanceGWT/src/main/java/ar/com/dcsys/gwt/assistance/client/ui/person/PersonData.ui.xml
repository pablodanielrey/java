<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
		.messages {
			width: 900px;
			height: 600px;
		}
	
	</ui:style>

	<g:FlowPanel>
		<g:Button ui:field="enroll" text="enrolar"/>
		<g:Button ui:field="persist" text="transferir"/>
		<g:FlowPanel styleName="${style.messages}">
			<g:VerticalPanel ui:field="messages" styleName="${style.messages}"/>
		</g:FlowPanel>
	</g:FlowPanel>

</ui:UiBinder> 