<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.contentMain {
			background-color: #E8EEF7;
			margin: 1em;
			-moz-border-radius: 5px;
			border-radius: 5px;
			border: 1px solid #C1C1C1;		
		}
		
		.button {
			margin-left:0.5em;
			color: #47698a;
			font-weight: bold;
		}
		
		.panel-button {
			margin: 0.5em;
			text-align:center;
		}
		
		.assistancePanel {
			margin-top:1em;
		}
		
	</ui:style>
	<g:FlowPanel styleName="{style.contentMain}">
		<g:FlowPanel ui:field="basicPersonData"/>
		<g:FlowPanel ui:field="modules"/>
		<g:FlowPanel styleName="{style.panel-button}">
			<g:Button ui:field="accept" text="Aceptar" styleName="{style.button}"/>
		</g:FlowPanel>
	</g:FlowPanel>

</ui:UiBinder> 