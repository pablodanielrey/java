<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:d='urn:import:com.google.gwt.user.datepicker.client'
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="res" type="ar.com.dcsys.gwt.mapau.client.ui.calendarv2.toolbar.ToolBarViewResources"/>
	
	<g:FlowPanel styleName="{res.style.toolbar}">
	    
	    <g:Label text="Mostrar Filtros" ui:field="showFilter" styleName="{res.style.filter}"/>
	    
	    <g:FlowPanel styleName="{res.style.separator}"/>
	    
	    <g:FlowPanel ui:field="datePanel" styleName="{res.style.datePanel}">
	    	<g:Label text="Fecha :" styleName="{res.style.dateLabel}"/>
	    	<d:DateBox ui:field="date" styleName="{res.style.date}"/>
	    </g:FlowPanel>
	    
	    <g:FlowPanel styleName="{res.style.separator}"/>
	     
	    <g:FlowPanel styleName="{res.style.calendarViewTypePanel}">
	    	<g:Label text="Vista del Calendario:" styleName="{res.style.calendarViewTypeLabel}"/>
	    	<g:ValueListBox ui:field="calendarViewType" styleName="{res.style.calendarViewType}"/>
	    </g:FlowPanel>
	</g:FlowPanel>
	
</ui:UiBinder> 