<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="res" type="ar.com.dcsys.gwt.menu.client.ui.MenuViewResources" />
	
	<g:FlowPanel styleName="{res.style.page}">
	
		<g:FlowPanel styleName="{res.style.panelPerson}" ui:field="panelPerson">
			<g:FlowPanel ui:field="personImageContainer" styleName="{res.style.personImageContainer}"/>
			<g:Label ui:field="person" styleName="{res.style.labelPerson}"/>
		</g:FlowPanel>
		
		<g:FlowPanel styleName="{res.style.panelAssistance}" ui:field="panelAssistance">
			<g:FlowPanel ui:field="assistanceImageContainer" styleName="{res.style.assistanceImageContainer}"/>
			<g:Label ui:field="assistance" styleName="{res.style.labelAssistance}"/>
		</g:FlowPanel>
		
		<g:FlowPanel styleName="{res.style.panelLogout}" ui:field="panelLogout">
			<g:FlowPanel ui:field="logoutImageContainer" styleName="{res.style.logoutImageContainer}"/>
			<g:Label ui:field="logout" styleName="{res.style.labelLogout}"/>
		</g:FlowPanel>
		
	</g:FlowPanel>
	
</ui:UiBinder> 